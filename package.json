{
  "name": "webdriverIO-project",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "remote": "npx /app/node_modules/.bin/wdio run /app/test/config/chrome.conf.js",
    "chrome": "npx wdio run ./test/config/chrome.conf.js",
    "spec": "npx /wdio  run ./test/config/chrome.conf.js --spec",
    "suite": "npx wdio run ./test/config/selenium.conf.js --suite",
    "selenium": "npx wdio run ./test/config/selenium.conf.js",
    "docker": "npx wdio run ./test/config/docker.conf.js",
    "allure:history": "mkdir -p allure-report/history ./test/reports/allure-results/history && cp -a allure-report/history ./test/reports/allure-results/",
    "allure-report": "node_modules/.bin/allure generate -c ./test/reports/allure-results && allure open",
    "e2e": "npm run chrome && npm run allure-report",
    "build":"docker build -t wdiotest -f Dockerfile .",
    "docker-run":"docker run -it wdiotest npm run remote"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/register": "^7.16.9",
    "@wdio/allure-reporter": "^7.16.14",
    "@wdio/cli": "^7.16.12",
    "@wdio/dot-reporter": "^7.16.14",
    "@wdio/junit-reporter": "^7.13.2",
    "@wdio/local-runner": "^7.16.13",
    "@wdio/mocha-framework": "^7.16.13",
    "@wdio/selenium-standalone-service": "^7.16.11",
    "@wdio/spec-reporter": "^7.16.11",
    "allure-commandline": "^2.13.8",
    "chai": "^4.3.4",
    "chromedriver": "^98.0.1",
    "dotenv": "^10.0.0",
    "mocha": "^9.1.3",
    "moment": "^2.29.1",
    "should": "^13.2.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2",
    "wdio-chromedriver-service": "^7.2.8",
    "wdio-json-reporter": "^3.0.0",
    "xlsx": "^0.17.3"
  }
}
